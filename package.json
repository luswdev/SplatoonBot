{
  "name": "spl3",
  "version": "2.3.3",
  "description": "Splatoon 3 Discord Bot",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "start": "node index.js",
    "cmd": "node deployCmds.js",
    "release": "pkg index.js -t node16-linux-x64 -o release/spl3 -c package.json",
    "install": "sudo cp release/spl3 /usr/bin/",
    "clean": "rm -rf release/* build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luswdev/splatoon-bot.git"
  },
  "keywords": [
    "Splatoon"
  ],
  "author": "LuSkywalker",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luswdev/splatoon-bot/issues"
  },
  "homepage": "https://github.com/luswdev/splatoon-bot#readme",
  "dependencies": {
    "discord.js": "^14.6.0",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "pkg": "^5.8.0"
  },
  "devDependencies": {
    "eslint": "^8.25.0"
  },
  "pkg": {
    "assets": "data/**/*.json*"
  }
}
